{
    "name": "eventum/slack-unfurl",
    "description": "Slack app for unfurling Eventum issue links",
    "license": "BSD",
    "type": "project",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Eventum\\SlackUnfurl\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "silex/silex": "~2.0"
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web"
        ]
    }
}
